<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Loghub name="Loghub"
                projectName = "ali-cn-shanghai-corp-sls-admin"
                logstore = "testappender"
                endpoint = "cn-shanghai-corp.sls.aliyuncs.com"
                accessKeyId = ""
                accessKey = "">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Loghub>
    </Appenders>
    <Loggers>
        <Logger name="slsLogger" level="warn">
            <AppenderRef ref="Loghub"/>
        </Logger>
        <Root level="warn">
            <AppenderRef ref="Loghub"/>
        </Root>
    </Loggers>
</Configuration>